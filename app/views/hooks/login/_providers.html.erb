<% OmniAuth::OpenIDConnect::Provider.available.each do |pro| %>
  <%
    opts = {
      :controller => '/auth',
      :action => pro.provider_name
    }
    if params["back_url"]
      opts[:origin] = params["back_url"]
    end
  %>
  //&nbsp;<%= link_to pro.provider_name.camelize, opts %>
<% end %>
